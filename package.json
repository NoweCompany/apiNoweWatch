{
  "name": "clear",
  "version": "1.0.0",
  "description": "NoweWatch - Plataforma de videos/vlogs da nowe",
  "main": "index.ts",
  "scripts": {
    "start": "dotenv -e .env.prod && node ./dist/index.js",
    "build": "tsc",
    "dev": "dotenv -e .env.prod && nodemon --exec ts-node ./src/index.ts",
    "test": "dotenv -e .env.test -- jest",
    "test:dev": "dotenv -e .env.test -- jest --watch",
    "migrate": "dotenv -e .env.prod prisma migrate dev"
  },
  "keywords": [
    "api"
  ],
  "author": "https://github.com/Moyseys",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "prisma": "^5.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.3",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
